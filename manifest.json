{
    "manifest_version": 3,
    "name": "吉时学助手",
    "version": "1.0.1",
    "description": "该插件主要用于吉时学平台考试使用",
    "permissions": [
        "activeTab",
        "tabs",
        "cookies",
        "webRequest"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}
